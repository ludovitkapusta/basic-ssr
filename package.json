{
  "name": "basic-ssr",
  "engines": {
    "node": "12.14.1"
  },
  "version": "1.0.0",
  "description": "React, Typescript, styled-components and webpack server side rendering app implementation for usage with ReactJS.NET",
  "scripts": {
    "build": "yarn clean:appData && yarn build:prod",
    "build:prod": "webpack --config webpack.prod.js",
    "build:net": "yarn clean:appData && webpack --config webpack.prod.net.js",
    "transpile:appData": "webpack --config webpack.appData.js",
    "clean:appData": "rimraf assets/local/appData.js",
    "dev": "yarn clean:appData && yarn transpile:appData && yarn dev:local",
    "dev:local": "webpack-dev-server --config webpack.local.js",
    "dev:build": "webpack --watch --config webpack.dev.js",
    "dev:server": "webpack --watch --config webpack.server.js",
    "ssr:client": "yarn clean:appData && concurrently \"yarn dev:build\" \"yarn dev:server\"",
    "ssr:server": "nodemon  ./assets/local/server.js",
    "lint": "eslint --ext=js,ts,tsx src --ext=js,ts,tsx client  --ext=js,ts,tsx server --ext=js,ts,tsx tests",
    "lint:styles": "stylelint \"src/**/*.tsx\"",
    "lint:fix": "yarn lint --fix",
    "jest": "jest"
  },
  "author": {
    "name": "Ludovit Kapusta",
    "email": "ludovit.kapusta@gmail.com"
  },
  "license": "ISC",
